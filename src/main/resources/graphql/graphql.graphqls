type Story {
    id: ID!
    name: String!
    genre: String!
    type: String!
    wordCount: Int!
    summary: String
}

input StoryInput {
    id: ID!
    name: String!
    genre: String!
    type: String!
    wordCount: Int!
    summary: String
}

type Query {
    getStories: [String]
    getStory(id: ID): String
    searchStory(text: String): [Story]
    searchStoryIds(text: String): [String]
}

type Mutation {
   putStory(storyInput: StoryInput): String
   deleteStory(id: ID): String
}
